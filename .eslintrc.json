{
	"extends": "./node_modules/pandora-common/.eslintrc.json",
	"env": {
		"node": true
	},
	"settings": {
		"import/parsers": {
			"@typescript-eslint/parser": [".ts", ".tsx"]
		},
		"import/resolver": {
			"typescript": {
				"alwaysTryTypes": true
			}
		}
	},
	"ignorePatterns": ["/.git", "/node_modules", "/dist", "/pandora"],
	"root": true
}
